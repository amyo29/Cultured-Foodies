                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      },
      "/restaurants": {
        "get": {
          "summary": "Returns list of restaurants",
          "operationId": "listRestaurants",
          "tags": ["restaurants"],
          "parameters": [
            {
              "name": "city",
              "in": "query",
              "description": "filter by city that restaurant is in",
              "required": false,
              "schema": {
                "type": "string",
                "format": "string"
              }
            },
            {
              "name": "cuisines",
              "in": "query",
              "description": "filter by cuisines for this restaurant",
              "required": false,
              "schema": {
                "type": "string",
                "format": "string"
              }
            },
            {
              "name": "price range",
              "in": "query",
              "description": "sort by price range of restaurant",
              "required": false,
              "schema": {
                "type": "string",
                "format": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "List of restaurants and their corresponding details",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Restaurant"
                  }
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      },
      "/restaurants/id=<id>": {
        "get": {
          "summary": "Details about a restaurant",
