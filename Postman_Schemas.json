{
    "openapi": "3.0.0",
    "info": {
      "version": "1.0.0",
      "title": "CulturedFoodies",
      "license": {
        "name": "MIT"
      }
    },
    "servers": [
      {
        "url": "https://culturedfoodies.me/api"
      }
    ],
    "paths": {
      "/countries": {
        "get": {
          "summary": "Returns list of countries",
          "operationId": "listCountries",
          "tags": ["countries"],
          "parameters": [
          ],
          "responses": {
            "200": {
              "description": "List of all countries and corresponding details",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Country"
                  }
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      },
      "/countries/{countryId}": {
        "get": {
          "summary": "Details about a country",
          "operationId": "listCountry",
          "tags": ["country"],
          "parameters": [
            {
              "name": "id",
              "in": "query",
              "description": "id of the country",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Details about a country",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Country"
                  }
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      },
      "/cities": {
        "get": {
          "summary": "Returns list of cities in the USA",
          "operationId": "listCities",
          "tags": ["cities"],
          "parameters": [
            {
              "name": "language",
              "in": "query",
              "description": "Filter by state of the city",
              "required": false,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "timezone",
              "in": "query",
              "description": "Filter by timezone of the city",
              "required": false,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "currency",
              "in": "query",
              "description": "Filter by population of the city",
              "required": false,
              "schema": {
                "type": "number"
              }
            },
            {
              "name": "sort",
              "in": "query",
              "description": "Can sort by name with sort=name. Adding a '-' in front returns in descending order",
              "required": false,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "List of all cities and corresponding details",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/City"
                  }
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      },
      "/cities/id=<id>": {
        "get": {
          "summary": "Details about a city",
          "operationId": "listCity",
          "tags": ["city"],
          "parameters": [
            {
              "name": "id",
              "in": "query",
              "description": "id of the city",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Details about a city",
              "content": {
                "application/json": {
