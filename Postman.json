{
    "openapi": "3.0.0",
    "info": {
      "version": "1.0.0",
      "title": "CulturedFoodies",
      "license": {
        "name": "MIT"
      }
    },
    "servers": [
      {
        "url": "https://culturedfoodies.me/api/"
      }
    ],
    "paths": {
      "/countries": {
        "get": {
          "summary": "Returns list of countries",
          "operationId": "listCountries",
          "tags": [
            "countries"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "query",
              "description": "ID of the user",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int32"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "List of all countries and corresponding details",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Countries"
                  }
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      },
      "/countries/{id}": {
        "get": {
          "summary": "Details about a user",
          "operationId": "listUser",
          "tags": [
            "user"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "query",
              "description": "ID of the user",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int32"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Details about a user",
              "headers": {
                "x-next": {
                  "description": "A link to the next page of responses",
                  "schema": {
                    "type": "string"
                  }
                }
              },
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      },
      "/dishes": {
        "get": {
          "summary": "Details about a user",
          "operationId": "listUser",
          "tags": [
            "user"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "query",
              "description": "ID of the user",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int32"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Details about a user",
              "headers": {
                "x-next": {
                  "description": "A link to the next page of responses",
                  "schema": {
                    "type": "string"
                  }
                }
              },
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      },
      "/news": {
        "get": {
          "summary": "Details about a user",
          "operationId": "listUser",
          "tags": [
            "user"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "query",
              "description": "ID of the user",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int32"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Details about a user",
              "headers": {
                "x-next": {
                  "description": "A link to the next page of responses",
                  "schema": {
                    "type": "string"
                  }
                }
              },
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            },
            "default": {
              "description": "unexpected error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Error"
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "Country": {
            "type": "object",
            "required": [
                "id",
                "name",
                "code",
                "language",
                "population",
                "timezone",
                "currency",
                "region",
                "flagUrl",
                "latlong"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "name": {
                    "type": "string"
                },
                "code": {
                    "type": "string"
                },
                "language": {
                    "type": "string"
                },
                "population": {
                    "type": "integer",
                    "format": "int64"
                },
                "timezone": {
                    "type": "string"
                },
                "currency": {
                    "type": "string"
                },
                "region": {
                    "type": "string"
                },
                "flagUrl": {
                    "type": "string"
                },
                "latlong": {
                    "type": "array",
                    "items": {
                        "type": "number",
                        "minimum": -180,
                        "maximum": 180
                    },
                    "minItems": 2,
                    "maxItems": 2
                }
            }
        },
        "Dishes": {
            "type": "object",
            "required": [
              "id",
              "name"
            ],
            "properties": {
              "id": {
                "type": "integer",
                "format": "int64"
              },
              "name": {
                "type": "string"
              },
              "tag": {
                "type": "string"
              }
            }
          },
          "News": {
            "type": "object",
            "required": [
              "id",
              "name"
            ],
            "properties": {
              "id": {
                "type": "integer",
                "format": "int64"
              },
              "name": {
                "type": "string"
              },
              "tag": {
                "type": "string"
              }
            }
          },
        "Error": {
          "type": "object",
          "required": [
            "code",
            "message"
          ],
          "properties": {
            "code": {
              "type": "integer",
              "format": "int32"
            },
            "message": {
              "type": "string"
            }
          }
        }
      }
    }
  }